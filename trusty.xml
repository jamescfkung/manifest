<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="aosp"
           fetch="https://android.googlesource.com"
           revision="master"
           review="https://android-review.googlesource.com/" />

  <project remote="aosp" path="linux" name="kernel/common" revision="android-trusty-3.18" />

  <!-- BEGIN trusty core projects -->
  <project remote="james" path="trusty_os/external/lk" name="trusty-lk-common.git" groups="trusty" >
    <copyfile src="makefile" dest="trusty_os/makefile" />
  </project>
  <project remote="aosp" path="trusty_os/external/headers" name="trusty/external/headers" groups="trusty" />
  <project remote="aosp" path="trusty_os/lib" name="trusty/lib" groups="trusty" />
  <project remote="james" path="trusty_os/lk/trusty" name="trusty-lk-trusty.git" groups="trusty" />
  <!-- END trusty core projects -->

  <!-- BEGIN apps project  -->
  <project remote="aosp" path="trusty_os/app/sample" name="trusty/app/sample" groups="trusty" />
  <!-- END apps project -->

  <!-- BEGIN trusty device projects -->
  <project remote="aosp" path="trusty_os/device/arm/generic-arm64" name="trusty/device/arm/generic-arm64" groups="trusty" />
  <project remote="aosp" path="trusty_os/device/arm/vexpress-a15" name="trusty/device/arm/vexpress-a15" groups="trusty" />
  <!-- END trusty device projects -->

  <!-- BEGIN trusty vendor projects -->
  <project remote="aosp" path="trusty_os/vendor/google/aosp" name="trusty/vendor/google/aosp" groups="trusty" >
   <!-- Install lk_inc.mk into trusty root -->
    <copyfile src="lk_inc.mk" dest="trusty_os/lk_inc.mk" />
  </project>
  <!-- END trusty vendor projects -->

  <!-- BEGIN toolchains -->
  <project remote="aosp" path="trusty_os/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.8" name="platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.8" groups="trusty" />
  <project remote="aosp" path="trusty_os/prebuilts/gcc/linux-x86/arm/arm-eabi-4.8" name="platform/prebuilts/gcc/linux-x86/arm/arm-eabi-4.8" groups="trusty" />
  <!-- END toolchains -->

  <!-- BEGIN aux external projects -->
  <project remote="aosp" path="trusty_os/external/qemu" name="trusty/external/qemu" groups="trusty" />
  <!-- END aux external projects -->

</manifest>
